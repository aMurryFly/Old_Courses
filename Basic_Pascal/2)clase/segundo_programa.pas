Program Segundo_programa;

//Recuerda que para la suma utilizas  {addition}

Var       
    Num1, Num2, Sum : Integer;

Begin 
	Writeln('Escribe el primer numero a sumar:'); 
	Readln(Num1);
	Writeln('Escribe el segundo numero:');
	Readln(Num2);
	Sum := Num1 + Num2; {addition} 
	Writeln(Sum);
	Readln;
End.      
